import java.net.InetAddress;


public class Main {
	public static void main(String[] args) {
		
	}
	
	public static String getMyIP() {  
	    String n=null;  
	    try  
	    {  
	        InetAddress ip=InetAddress.getLocalHost();  
	        String n=ip.toString();  
	          
	    }catch(Exception e){e.printStackTrace();}  
	    return n;  
	}  
}

